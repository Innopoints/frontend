{
  "name": "innopoints-frontend",
  "description": "Innopolis University innopoints project",
  "version": "0.4.0",
  "engines": {
    "node": "v12.14.0"
  },
  "scripts": {
    "dev": "sapper dev",
    "build": "sapper build --legacy",
    "export": "sapper export --legacy",
    "start": "node __sapper__/build",
    "lint": "eslint --format=pretty .",
    "lint:fix": "eslint --format=pretty --fix ."
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --format=pretty",
      "eslint --max-warnings 0 --fix",
      "git add"
    ],
    "*.svelte": [
      "eslint --format=pretty",
      "eslint --max-warnings 0 --fix",
      "git add"
    ]
  },
  "dependencies": {
    "compression": "^1.7.1",
    "polka": "next",
    "sirv": "^0.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.4.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-formatter-pretty": "^2.1.1",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-svelte3": "^2.7.3",
    "fecha": "^3.0.3",
    "husky": "^3.0.5",
    "lint-staged": "^9.2.5",
    "npm-run-all": "^4.1.5",
    "rollup": "^1.12.0",
    "rollup-plugin-alias": "^2.0.0",
    "rollup-plugin-babel": "^4.0.2",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-svelte": "^5.0.1",
    "rollup-plugin-terser": "^4.0.4",
    "sapper": "^0.27.9",
    "simplebar": "^5.0.7",
    "sortablejs": "^1.10.2",
    "svelte": "^3.14.0",
    "swiper": "^5.2.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VanishMax/innopoints-frontend.git"
  },
  "license": "MIT"
}
