{
  "name": "innopoints-frontend",
  "description": "Innopolis University innopoints project",
  "version": "0.4.0",
  "scripts": {
    "dev": "sapper dev",
    "build": "sapper build --legacy",
    "export": "sapper export --legacy",
    "start": "node __sapper__/build",
    "lint": "eslint --format=pretty .",
    "lint:fix": "eslint --format=pretty --fix ."
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --format=pretty",
      "eslint --max-warnings 0 --fix",
      "git add"
    ],
    "*.svelte": [
      "eslint --format=pretty",
      "eslint --max-warnings 0 --fix",
      "git add"
    ]
  },
  "dependencies": {
    "compression": "^1.7.1",
    "polka": "^1.0.0-next.11",
    "sapper-environment": "^1.0.1",
    "simplebar": "^5.1.0",
    "sirv": "^0.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/runtime": "^7.8.3",
    "@rollup/plugin-alias": "^3.0.0",
    "@rollup/plugin-commonjs": "^11.0.1",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "@rollup/plugin-replace": "^2.3.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.4.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-formatter-pretty": "^2.1.1",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-svelte3": "^2.7.3",
    "fecha": "^3.0.3",
    "husky": "^3.0.5",
    "lint-staged": "^9.2.5",
    "npm-run-all": "^4.1.5",
    "rollup": "^1.29.1",
    "rollup-plugin-babel": "^4.0.2",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-svelte": "^5.0.1",
    "rollup-plugin-terser": "^4.0.4",
    "sapper": "^0.27.9",
    "sortablejs": "^1.10.2",
    "stylelint": "^12.0.1",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-scss": "^3.13.0",
    "svelte": "^3.18.0",
    "swiper": "^5.2.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VanishMax/innopoints-frontend.git"
  },
  "license": "MIT"
}
