{
  "name": "innopoints-frontend",
  "description": "Innopolis University innopoints project",
  "version": "4.0.0",
  "scripts": {
    "dev": "sapper dev",
    "build": "sapper build --legacy",
    "export": "sapper export --legacy",
    "start": "node __sapper__/build",
    "lint": "eslint --format=pretty . --ext '.svelte,.js'",
    "lint:fix": "eslint --format=pretty --fix . --ext '.svelte,.js'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "[^.]*.js": [
      "eslint --format=pretty",
      "eslint --max-warnings 0 --fix",
      "git add"
    ],
    "*.svelte": [
      "eslint --format=pretty",
      "eslint --max-warnings 0 --fix",
      "git add"
    ]
  },
  "browserslist": [
    "last 2 versions",
    "not dead",
    "> 0.2%"
  ],
  "dependencies": {
    "clipboard-polyfill": "^3.0.0-pre5",
    "compression": "^1.7.1",
    "express": "^4.17.1",
    "helmet": "^4.2.0",
    "node-fetch": "^2.6.1",
    "sapper-environment": "^1.0.1",
    "simplebar": "^5.3.0",
    "sirv": "^1.0.9"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "@babel/runtime": "^7.12.5",
    "@beyonk/svelte-carousel": "^2.8.0",
    "@rollup/plugin-alias": "^3.0.0",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@rollup/plugin-replace": "^2.3.4",
    "@sveltejs/eslint-config": "sveltejs/eslint-config",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "attractions": "^2.2.3",
    "autoprefixer": "^10.0.4",
    "babel-eslint": "^10.0.3",
    "eslint": "^7.15.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-formatter-pretty": "^4.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-svelte3": "^2.7.3",
    "husky": "^4.3.4",
    "lint-staged": "^10.5.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.1.14",
    "rollup": "^2.34.1",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "sapper": "^0.28.10",
    "sass": "^1.32.2",
    "stylelint": "^13.8.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.16.1",
    "svelte": "^3.31.0",
    "svelte-easy-crop": "^1.0.3",
    "svelte-preprocess": "^4.6.1",
    "typescript": "^4.1.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VanishMax/innopoints-frontend.git"
  },
  "license": "MIT"
}
