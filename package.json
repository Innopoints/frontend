{
  "name": "innopoints-frontend",
  "description": "Innopolis University innopoints project",
  "version": "0.4.0",
  "scripts": {
    "dev": "sapper dev",
    "build": "sapper build --legacy",
    "export": "sapper export --legacy",
    "start": "node __sapper__/build",
    "lint": "eslint --format=pretty .",
    "lint:fix": "eslint --format=pretty --fix ."
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --format=pretty",
      "eslint --max-warnings 0 --fix",
      "git add"
    ],
    "*.svelte": [
      "eslint --format=pretty",
      "eslint --max-warnings 0 --fix",
      "git add"
    ]
  },
  "dependencies": {
    "clipboard-polyfill": "^2.8.6",
    "compression": "^1.7.1",
    "express": "^4.17.1",
    "helmet": "^3.22.0",
    "node-fetch": "^2.6.0",
    "sapper-environment": "^1.0.1",
    "simplebar": "^5.1.0",
    "sirv": "^0.4.0",
    "uuid": "^7.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/runtime": "^7.9.2",
    "@rollup/plugin-alias": "^3.0.0",
    "@rollup/plugin-commonjs": "^11.0.1",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "@rollup/plugin-replace": "^2.3.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.4.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-formatter-pretty": "^3.0.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-svelte3": "^2.7.3",
    "fecha": "^4.2.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.1.3",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.6.0",
    "rollup-plugin-babel": "^4.0.2",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-svelte": "^5.2.1",
    "rollup-plugin-terser": "^5.3.0",
    "sapper": "^0.27.12",
    "sortablejs": "^1.10.2",
    "stylelint": "^13.3.1",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.16.1",
    "svelte": "^3.18.0",
    "svelte-easy-crop": "^1.0.3",
    "swiper": "^5.3.7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VanishMax/innopoints-frontend.git"
  },
  "browserslist": [
    "last 2 versions, not dead, > 0.1%"
  ],
  "license": "MIT"
}
