{
  "name": "innopoints-frontend",
  "description": "Innopolis University innopoints project",
  "version": "4.0.0",
  "type": "module",
  "scripts": {
    "dev": "sapper dev",
    "build": "sapper build --legacy",
    "export": "sapper export --legacy",
    "start": "node __sapper__/build",
    "lint": "eslint --format=pretty . --ext '.svelte,.js'",
    "lint:fix": "eslint --format=pretty --fix . --ext '.svelte,.js'",
    "prepare": "husky install"
  },
  "lint-staged": {
    "[^.]*.js": [
      "eslint --format=pretty",
      "eslint --max-warnings 0 --fix",
      "git add"
    ],
    "*.svelte": [
      "eslint --format=pretty",
      "eslint --max-warnings 0 --fix",
      "git add"
    ]
  },
  "browserslist": [
    "last 2 versions",
    "not dead",
    "> 0.2%"
  ],
  "dependencies": {
    "clipboard-polyfill": "^3.0.2",
    "compression": "^1.7.1",
    "express": "^4.17.1",
    "helmet": "^4.4.1",
    "node-fetch": "^2.6.7",
    "sapper-environment": "^1.0.1",
    "simplebar": "^5.3.0",
    "sirv": "^1.0.11"
  },
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.13.8",
    "@babel/preset-env": "^7.13.8",
    "@babel/runtime": "^7.13.8",
    "@beyonk/svelte-carousel": "^2.8.0",
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@rollup/plugin-replace": "^2.4.1",
    "@sveltejs/eslint-config": "sveltejs/eslint-config",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "attractions": "^3.1.0",
    "autoprefixer": "^10.2.4",
    "babel-eslint": "^10.0.3",
    "eslint": "^7.21.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-formatter-pretty": "^4.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-svelte3": "^3.1.2",
    "husky": "^5.1.2",
    "lint-staged": "^10.5.4",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.13",
    "rollup": "^2.40.0",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "sapper": "^0.29.1",
    "sass": "^1.32.8",
    "stylelint": "^13.11.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.19.0",
    "svelte": "^3.49.0",
    "svelte-easy-crop": "^1.0.3",
    "svelte-preprocess": "^4.6.9",
    "typescript": "^4.1.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Innopoints/frontend.git"
  },
  "license": "MIT"
}
